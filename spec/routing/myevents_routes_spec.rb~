require "spec_helper"
describe MyeventsController do
 describe "routing" do
  it "routes to #index" do
    get("/myevents/index").should route_to("myevents#index")

  end
   it "routes to #adminindex" do
    get("/myevents/adminindex").should route_to("myevents#adminindex")

  end

  it "routes to #event" do
    get("/myevents/event").should route_to("myevents#event")

  end
  it "routes to #cause" do
    get("/myevents/cause").should route_to("myevents#cause")

  end
    it "routes to #template" do
    get("/myevents/template").should route_to("myevents#template")

  end
    it "routes to #education" do
    get("/myevents/education").should route_to("myevents#education")

  end
     it "routes to #pollution" do
    get("/myevents/pollution").should route_to("myevents#pollution")

  end
    it "routes to #food" do
    get("/myevents/food").should route_to("myevents#food")

  end
    it "routes to #environment" do
    get("/myevents/environment").should route_to("myevents#environment")

  end
   
  it "routes to #message" do
    get("/myevents/message").should route_to("myevents#message")

  end

 end
end




