require "spec_helper"
describe HomeController do
 describe "routing" do
  it "routes to #aboutus" do
    get("/home/aboutus").should route_to("home#aboutus")
puts "1"
  end
   it "routes to #aft_login" do
    get("/home/aft_login").should route_to("home#aft_login")
puts "2"
  end
  it "routes to #admin_login" do
    get("/home/admin_login").should route_to("home#admin_login")
puts "3"
  end
  it "routes to #works" do
    get("/home/works").should route_to("home#works")
puts "4"
  end
  it "routes to #term" do
    get("/home/term").should route_to("home#term")
  puts "5"
  end
  it "routes to #term" do
    get("/home/term").should route_to("home#term")
puts "6"
  end
 
 end
end




